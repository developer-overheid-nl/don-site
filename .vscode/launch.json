{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Start Docusaurus",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "pnpm",
            "runtimeArgs": [
                "start:docusaurus",
                "--host",
                "0.0.0.0"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        }
    ]
}